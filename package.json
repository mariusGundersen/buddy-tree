{
  "name": "buddy-tree",
  "version": "2.0.2",
  "description": "Immutable buddy memory allocation",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "module": "lib/main.js",
  "scripts": {
    "clean": "rm -rf ./lib ./dist",
    "test": "ava",
    "typescript": "tsc",
    "compile": "npm run typescript && npm run babel",
    "babel": "babel --source-maps --copy-files -d dist lib",
    "watch": "tsc -w & ava -w",
    "prepublish": "npm run clean && npm run compile && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mariusGundersen/buddy-tree.git"
  },
  "keywords": [
    "immutable",
    "memory",
    "buddy"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mariusGundersen/buddy-tree/issues"
  },
  "homepage": "https://github.com/mariusGundersen/buddy-tree#readme",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.22.2",
    "babel-preset-es2015": "^6.22.0",
    "typescript": "^2.1.5"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "source": [
      "lib/*.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "peerDependencies": {
    "babel-runtime": "^6.22.0"
  }
}
