  },
  "keywords": [
    "immutable",
    "memory",
    "buddy"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mariusGundersen/buddy-tree/issues"
  },
  "homepage": "https://github.com/mariusGundersen/buddy-tree#readme",
  "devDependencies": {
    "ava": "^0.16.0",
    "typescript": "^2.1.5"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "source": [
      "src/*.ts"
    ]
  }
