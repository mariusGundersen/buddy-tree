{
  "name": "buddy-tree",
  "version": "2.4.0",
  "description": "Immutable buddy memory allocation",
  "main": "js/main.js",
  "types": "es/main.d.ts",
  "module": "es/main.js",
  "scripts": {
    "test": "ava",
    "clean": "rimraf ./es ./js",
    "tsc": "tsc",
    "babel": "babel --source-maps -d js es",
    "compile": "npm run tsc && npm run babel",
    "watch": "ava -w",
    "prepublish": "npm run clean && npm run test && npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mariusGundersen/buddy-tree.git"
  },
  "keywords": [
    "immutable",
    "memory",
    "buddy"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mariusGundersen/buddy-tree/issues"
  },
  "homepage": "https://github.com/mariusGundersen/buddy-tree#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@types/node": "^12.6.9",
    "ava": "^2.2.0",
    "esm": "^3.2.25",
    "rimraf": "^2.6.3",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.5.5"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "files": [
      "./ts/**/*.test.ts"
    ],
    "require": [
      "esm",
      "ts-node/register"
    ]
  }
}
