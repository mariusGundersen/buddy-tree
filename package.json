{
  "name": "buddy-tree",
  "version": "2.3.0",
  "description": "Immutable buddy memory allocation",
  "main": "js/main.js",
  "types": "es/main.d.ts",
  "module": "es/main.js",
  "scripts": {
    "clean": "rm -rf ./es ./js",
    "test": "ava",
    "tsc": "tsc",
    "compile": "npm run tsc && npm run babel",
    "babel": "babel --source-maps -d js es",
    "watch": "tsc -w & ava -w",
    "prepublish": "npm run clean && npm run compile && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mariusGundersen/buddy-tree.git"
  },
  "keywords": [
    "immutable",
    "memory",
    "buddy"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mariusGundersen/buddy-tree/issues"
  },
  "homepage": "https://github.com/mariusGundersen/buddy-tree#readme",
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "typescript": "^2.9.1"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "source": [
      "es/*.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "peerDependencies": {
    "babel-runtime": "^6.22.0"
  }
}
